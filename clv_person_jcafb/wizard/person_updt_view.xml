<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="clv_person_updt_view_form_jcafb" model="ir.ui.view">
            <field name="name">clv.person.updt.form</field>
            <field name="model">clv.person.updt</field>
            <field name="inherit_id" ref="clv_person.clv_person_updt_view_form"/>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <group name="global_tag_ids_selection" position="before">
                    <group>
                        <field name="reg_state_selection"/>
                    </group>
                    <group>
                        <field name="reg_state" nolabel='1'/>
                    </group>
                    <group>
                        <field name="state_selection"/>
                    </group>
                    <group>
                        <field name="state" nolabel='1'/>
                    </group>
                </group>
                <group name="category_ids" position="after">
                    <group>
                        <field name="employee_id_selection"/>
                    </group>
                    <group>
                        <field name="employee_id" nolabel='1'/>
                    </group>
                    <group>
                        <field name="random_field_selection"/>
                    </group>
                    <group>
                        <field name="random_field" nolabel='1'/>
                    </group>
                </group>
            </field>
        </record>

    </data>
</odoo>
