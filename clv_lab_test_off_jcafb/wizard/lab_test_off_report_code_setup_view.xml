<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="clv_lab_test_off_report_code_setup_view_form" model="ir.ui.view">
            <field name="name">clv.lab_test.off.report.code.setup.form</field>
            <field name="model">clv.lab_test.off.report.code.setup</field>
            <field name="arch" type="xml">
                <form>
                    <div class="oe_right">
                    </div>
                    <group>
                        <group>
                            <field name="off_instance_id" options="{'no_open': True, 'no_create': True}"/>
                            <field name="quantity"/>
                        </group>
                    </group>
                    <footer>
                        <button 
                            type="object"
                            name="do_lab_test_off_report_code_setup"
                            string="Lab Test (Off) Report Codes Set Up"
                            class="oe_highlight"
                            />
                        <button special="cancel" string="Cancel"/>
                    </footer>
                </form>

            </field>
        </record>

        <!-- More button Action -->
        <act_window 
            id="clv_lab_test_off_report_code_setup_action"
            name="Lab Test (Off) Report Codes Set Up"
            src_model="clv.lab_test.off.report.code"
            res_model="clv.lab_test.off.report.code.setup"
            view_mode="form, tree"
            key2="client_action_multi"
            target="new"
        />

    </data>
</odoo>
